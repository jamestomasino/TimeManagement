package{	import flash.events.Event;		public class BoxEvent extends Event	{		public static const ROLL_OVER:String = 'box_roll_over';		public static const ROLL_OUT:String = 'box_roll_out';		public static const CLICK:String = 'box_click';				public var box:BoxItem;				public function BoxEvent ( type:String, bubbles:Boolean = true, cancelable:Boolean = true )		{			super (type, bubbles, cancelable);		}				override public function clone ():Event		{			var be:BoxEvent = new BoxEvent (type, bubbles, cancelable);			be.box = box;			return be;		}	}}